}
function show_prop(){
	if (explorer.ac_item != "") {
		$('item').update(explorer.ac_item);
//		$('prop','prop_toolbar').invoke('show');
		$('prop_toolbar').show();
		$('details').show();
//		$$('.detail_title','.detail_content').invoke('show');
	}
	else {
